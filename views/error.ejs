<% layout('layouts/boilerplate') %>
<div class="alert alert-success" role="alert">
    <h4 class="alert-heading">Error <%= statusCode %> - <%= err.message %></h4>
    <% if (process.env.NODE_ENV !== "production") { %>
    <p><%= err.stack %></p>
    <% } %>
    <hr />
    <p class="mb-0"></p>
</div>
